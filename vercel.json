{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["swagger/swagger.json", "./node_modules/swagger-ui-dist/**"]
            }
        }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "/index.js" },
        { "src": "/api-docs/(.*)", "dest": "/index.js" },
        { "src": "/api-docs", "dest": "/index.js" },
        { "src": "/swagger.json", "dest": "/index.js" },
        { "src": "/", "dest": "/index.js" }
    ]
}
