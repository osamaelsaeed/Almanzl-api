{
    "/orders/get-paginated-orders": {
        "get": {
            "tags": ["Orders"],
            "summary": "Get paginated orders",
            "description": "Retrieves a paginated list of orders.",
            "operationId": "getAllOrdersPaginated",
            "security": [{ "bearerAuth": [] }],
            "parameters": [
                {
                    "name": "page",
                    "in": "query",
                    "schema": { "type": "integer", "example": 1 }
                },
                {
                    "name": "limit",
                    "in": "query",
                    "schema": { "type": "integer", "example": 10 }
                }
            ],
            "responses": {
                "200": { "description": "Paginated orders retrieved successfully" },
                "401": { "description": "Unauthorized" }
            }
        }
    }
}
